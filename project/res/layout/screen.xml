<?xml version="1.0" encoding="utf-8"?>
<merge
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:ScreenView="http://schemas.android.com/apk/res/name.osher.gil.minivmac.ii"
    xmlns:Keyboard="http://schemas.android.com/apk/res/name.osher.gil.minivmac.ii">
    <name.osher.gil.minivmac.ii.ScreenView
    	android:id="@+id/screen"
		android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
        
    <name.osher.gil.minivmac.ii.Keyboard
    	android:id="@+id/keyboard"
    	android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom"
        android:paddingTop="8dip"
        android:gravity="center_horizontal"
        android:background="#AA000000"
        android:visibility="invisible" />
</merge>